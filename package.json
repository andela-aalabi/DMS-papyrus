{
  "name": "papyrus",
  "version": "1.0.0",
  "description": "Document Management System",
  "main": "index.jsx",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "start": "nodemon server.js --exec babel-node",
    "coverage-report": "cat ./coverage/lcov.info | coveralls",
    "undo:test": "NODE_ENV=test sequelize db:migrate:undo:all",
    "redo:test": "NODE_ENV=test sequelize db:migrate",
    "test:db": "npm run undo:test && npm run redo:test",
    "test": " npm run test:db && NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=html mocha \"tests/server/routes/*.spec.js\" --timeout 20000 && npm run coverage-report",
    "undo": "NODE_ENV=development sequelize db:seed:undo:all && sequelize db:migrate:undo:all",
    "redo": "NODE_ENV=development sequelize db:migrate && sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-aalabi/DMS-papyrus.git"
  },
  "keywords": [
    "javascript"
  ],
  "author": "Adeyinka Alabi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-aalabi/DMS-papyrus/issues"
  },
  "homepage": "https://github.com/andela-aalabi/DMS-papyrus#readme",
